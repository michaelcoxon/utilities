{
  "name": "@michaelcoxon/utilities",
  "version": "1.3.4",
  "description": "utilities for JS and TypeScript",
  "main": "dist/index.js",
  "module": "lib.2015/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "run-tests": "mocha -r ts-node/register tests/**/*.spec.ts",
    "test": "npm-run-all -s build-dev build-prod -p run-tests",
    "build-dev": "SET NODE_ENV=development&& webpack --no-color",
    "build-prod": "SET NODE_ENV=production&& webpack --no-color",
    "prepack": "npm-run-all -s test",
    "build-doc": "typedoc --out docs src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaelcoxon/utilities.git"
  },
  "keywords": [
    "utilities",
    "typescript"
  ],
  "author": "Michael Coxon",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/michaelcoxon/utilities/issues"
  },
  "homepage": "https://github.com/michaelcoxon/utilities#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.40",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/preset-typescript": "^7.0.0-beta.40",
    "@types/chai": "^4.0.10",
    "@types/mocha": "^2.2.44",
    "@types/prop-types": "^15.5.2",
    "@types/sinon": "^4.3.1",
    "@types/sinon-chai": "^2.7.29",
    "@types/webpack-env": "^1.13.5",
    "awesome-typescript-loader": "^3.4.1",
    "chai": "^4.1.2",
    "dts-bundle": "^0.7.3",
    "file-loader": "^0.11.2",
    "jsdoc-babel": "^0.4.0-alpha.0",
    "jsdoc-to-markdown": "^4.0.1",
    "json-loader": "^0.5.7",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.2",
    "sinon": "^5.0.7",
    "sinon-chai": "^3.0.0",
    "ts-node": "^4.0.2",
    "typedoc": "^0.11.1",
    "typescript": "^2.7.2",
    "webpack": "^2.7.0"
  },
  "-vs-binding": {
    "ProjectOpened": [
      "test"
    ]
  }
}
