{
  "name": "@michaelcoxon/utilities",
  "version": "1.8.2",
  "description": "utilities for JS and TypeScript",
  "main": "dist/index.js",
  "types": "lib/index.d.ts",
  "module": "lib/index.js",
  "sideEffects": false,
  "scripts": {
    "build-typescript": "tsc -p src\\config\\es5\\tsconfig.json&& tsc -p src\\config\\es6\\tsconfig.json&& tsc -p src\\config\\esnext\\tsconfig.json",
    "version": "npm -v && node -v",
    "test": "tsc && nyc --reporter=html --reporter=text mocha --opts ./mocha.opts",
    "build-dev": "SET NODE_ENV=development&& webpack",
    "build-prod": "SET NODE_ENV=production&& webpack",
    "prepack": "npm-run-all -s build-typescript build-prod test build-doc",
    "build-doc": "typedoc --tsconfig \"src\\config\\esnext\\tsconfig.json\" --logger console --out docs src&& copy .nojekyll docs\\.nojekyll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaelcoxon/utilities.git"
  },
  "keywords": [
    "utilities",
    "typescript"
  ],
  "author": "Michael Coxon",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/michaelcoxon/utilities/issues"
  },
  "homepage": "https://github.com/michaelcoxon/utilities#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/chai": "^4.2.0",
    "@types/core-js": "^2.5.2",
    "@types/mocha": "5.0.0",
    "@types/prop-types": "^15.7.1",
    "@types/shelljs": "^0.8.5",
    "@types/sinon": "^7.0.13",
    "@types/sinon-chai": "^3.2.3",
    "@types/webpack-env": "^1.14.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "8.0.6",
    "chai": "^4.2.0",
    "core-js": "^3.2.1",
    "dts-bundle": "^0.7.3",
    "file-loader": "^4.2.0",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^5.0.0",
    "json-loader": "^0.5.7",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "sinon": "^7.4.1",
    "sinon-chai": "^3.3.0",
    "source-map-support": "^0.5.13",
    "string-replace-loader": "^2.2.0",
    "ts-mocha": "^6.0.0",
    "ts-nameof": "^3.2.0",
    "ts-node": "^8.3.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-cyclic-dependency-checker": "^0.0.1",
    "webpack-log": "^3.0.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "tslib": "^1.10.0"
  },
  "-vs-binding": {
    "ProjectOpened": [
      "test"
    ]
  }
}
