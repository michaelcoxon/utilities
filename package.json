{
  "name": "@michaelcoxon/utilities",
  "version": "1.8.1",
  "description": "utilities for JS and TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "lib/index.js",
  "sideEffects": false,
  "scripts": {
    "build-typescript": "tsc -p src\\config\\es5\\tsconfig.json&& tsc -p src\\config\\es6\\tsconfig.json&& tsc -p src\\config\\es2015\\tsconfig.json",
    "version": "npm -v && node -v",
    "test": "mocha -r ts-node/register tests/**/*.spec.ts",
    "build-dev": "SET NODE_ENV=development&& webpack --no-color&& del lib\\index.d.ts",
    "build-prod": "SET NODE_ENV=production&& webpack --no-color&& del lib\\index.d.ts",
    "prepack": "npm-run-all -s build-typescript build-prod test build-doc",
    "build-doc": "typedoc --tsconfig \"src\\config\\es5\\tsconfig.json\" --logger console --out docs src&& copy .nojekyll docs\\.nojekyll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaelcoxon/utilities.git"
  },
  "keywords": [
    "utilities",
    "typescript"
  ],
  "author": "Michael Coxon",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/michaelcoxon/utilities/issues"
  },
  "homepage": "https://github.com/michaelcoxon/utilities#readme",
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-typescript": "7.1.0",
    "@types/chai": "4.1.6",
    "@types/mocha": "5.2.5",
    "@types/prop-types": "15.5.6",
    "@types/sinon": "5.0.5",
    "@types/sinon-chai": "3.2.0",
    "@types/webpack-env": "1.13.6",
    "awesome-typescript-loader": "5.2.1",
    "chai": "4.2.0",
    "dts-bundle": "0.7.3",
    "file-loader": "2.0.0",
    "jsdoc-babel": "0.5.0",
    "jsdoc-to-markdown": "4.0.1",
    "json-loader": "0.5.7",
    "mocha": "5.2.0",
    "npm-run-all": "4.1.3",
    "sinon": "6.3.5",
    "sinon-chai": "3.2.0",
    "ts-node": "^8.0.0",
    "typedoc": "0.13.0-0",
    "typescript": "3.3.3",
    "uglifyjs-webpack-plugin": "2.0.1",
    "webpack": "4.20.2",
    "webpack-log": "2.0.0",
    "webpack-cli": "3.1.2",
    "webpack-cyclic-dependency-checker": "0.0.1"
  },
  "dependencies": {
    "tslib": "1.9.3"
  },
  "-vs-binding": {
    "ProjectOpened": [
      "test"
    ]
  }
}
