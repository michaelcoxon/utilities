{
  "name": "@michaelcoxon/utilities",
  "version": "10.1.0",
  "description": "utilities for JS and TypeScript",
  "main": "dist/index.js",
  "types": "lib/index.d.ts",
  "module": "lib/index.js",
  "sideEffects": false,
  "scripts": {
    "_build:debug": "SET NODE_ENV=development&& webpack",
    "_build:release": "SET NODE_ENV=production&& webpack",
    "_build:typescript": "tsc",
    "build": "npm-run-all -s build:typescript -p _build:release",
    "build:debug": "npm-run-all -s build:typescript -p _build:debug",
    "build:typescript": "npm-run-all -s clean:lib _build:typescript",
    "clean:lib": "rimraf lib",
    "clean:dist": "rimraf dist",
    "clean": "npm-run-all -p clean:lib clean:dist",
    "test": "jest --coverage --coverageDirectory coverage --coverageReporters html",
    "build-doc": "typedoc --logger console --out docs src&& copy .nojekyll docs\\.nojekyll",
    "prepack": "npm-run-all -s build -p test build-doc",
    "webpack-stats":"SET NODE_ENV=production&& webpack --profile --json > webpack-stats.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaelcoxon/utilities.git"
  },
  "keywords": [
    "utilities",
    "typescript",
    "collection",
    "enumerable",
    "linq",
    "enumerator",
    "ienumerable",
    "list",
    "array",
    "comparer",
    "exception",
    "logging",
    "format"
  ],
  "author": "Michael Coxon",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/michaelcoxon/utilities/issues"
  },
  "homepage": "https://github.com/michaelcoxon/utilities#readme",
  "devDependencies": {
    "@types/jest": "27.0.1",
    "@types/node": "^14.17.14",
    "@types/prop-types": "^15.7.4",
    "@types/shelljs": "^0.8.9",
    "@types/webpack-env": "^1.16.2",
    "ajv": "^6.9.1",
    "dts-bundle": "^0.7.3",
    "file-loader": "^6.2.0",
    "jest": "27.0.3",
    "json-loader": "^0.5.7",
    "lodash-es": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "source-map-support": "^0.5.20",
    "string-replace-loader": "^3.0.3",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typedoc": "^0.22.3",
    "typescript": "^4.4.3",
    "webpack": "^5.52.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0",
    "webpack-cyclic-dependency-checker": "^0.0.1",
    "webpack-log": "^3.0.2"
  },
  "dependencies": {
    "tslib": "^2.3.1"
  }
}
