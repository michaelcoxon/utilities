{
  "name": "@michaelcoxon/utilities",
  "version": "10.0.0-alpha.3",
  "description": "utilities for JS and TypeScript",
  "main": "dist/index.js",
  "types": "lib/index.d.ts",
  "module": "lib/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "npm-run-all -s build-typescript build-prod",
    "version": "npm -v && node -v",
    "coverage": "jest --coverage --coverageDirectory coverage --coverageReporters html",
    "test": "jest",
    "build-dev": "SET NODE_ENV=development&& webpack",
    "build-doc": "typedoc --logger console --out docs src&& copy .nojekyll docs\\.nojekyll",
    "build-prod": "SET NODE_ENV=production&& webpack",
    "build-typescript": "tsc",
    "prepack": "npm-run-all -s build test build-doc"
  },
  "mocha": {
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "recursive": true
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaelcoxon/utilities.git"
  },
  "keywords": [
    "utilities",
    "typescript"
  ],
  "author": "Michael Coxon",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/michaelcoxon/utilities/issues"
  },
  "homepage": "https://github.com/michaelcoxon/utilities#readme",
  "devDependencies": {
    "@types/jest": "27.0.1",
    "@types/node": "^14.17.14",
    "@types/prop-types": "^15.7.4",
    "@types/shelljs": "^0.8.9",
    "@types/webpack-env": "^1.16.2",
    "awesome-typescript-loader": "^5.2.1",
    "dts-bundle": "^0.7.3",
    "file-loader": "^6.2.0",
    "jest": "27.0.3",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^7.0.1",
    "json-loader": "^0.5.7",
    "npm-run-all": "^4.1.5",
    "source-map-support": "^0.5.20",
    "string-replace-loader": "^2.3.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typedoc": "^0.22.3",
    "typescript": "^4.4.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.52.1",
    "webpack-cli": "^3.3.12",
    "webpack-cyclic-dependency-checker": "^0.0.1",
    "webpack-log": "^3.0.2"
  },
  "dependencies": {
    "tslib": "^2.3.1"
  },
  "-vs-binding": {
    "ProjectOpened": [
      "test"
    ]
  }
}
