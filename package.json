{
  "name": "@michaelcoxon/utilities",
  "version": "2.0.0-alpha",
  "description": "utilities for JS and TypeScript",
  "main": "dist/index.js",
  "types": "lib/index.d.ts",
  "module": "lib/index.js",
  "sideEffects": false,
  "scripts": {
    "build-typescript": "tsc -p src\\config\\es5\\tsconfig.json&& tsc -p src\\config\\es6\\tsconfig.json&& tsc -p src\\config\\esnext\\tsconfig.json",
    "version": "npm -v && node -v",
    "coverage": "nyc --reporter=html --reporter=text mocha ./test/**/*.spec.ts",
    "test": "mocha ./test/**/*.spec.ts",
    "test-fast": "SET SKIP_SLOW_TESTS=true&& npm test",
    "build-dev": "SET NODE_ENV=development&& webpack",
    "build-prod": "SET NODE_ENV=production&& webpack",
    "prepack": "npm-run-all -s build-typescript build-prod test",
    "build-doc": "typedoc --tsconfig \"src\\config\\esnext\\tsconfig.json\" --logger console --out docs src&& copy .nojekyll docs\\.nojekyll"
  },
  "mocha": {
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "recursive": true
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaelcoxon/utilities.git"
  },
  "keywords": [
    "utilities",
    "typescript"
  ],
  "author": "Michael Coxon",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/michaelcoxon/utilities/issues"
  },
  "homepage": "https://github.com/michaelcoxon/utilities#readme",
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.17",
    "@types/core-js": "^2.5.4",
    "@types/mocha": "^8.2.2",
    "@types/node": "^12.20.10",
    "@types/prop-types": "^15.7.3",
    "@types/shelljs": "^0.8.8",
    "@types/sinon": "^9.0.11",
    "@types/sinon-chai": "^3.2.5",
    "@types/webpack-env": "^1.16.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.2.2",
    "chai": "^4.3.4",
    "core-js": "^3.11.0",
    "dts-bundle": "^0.7.3",
    "file-loader": "^6.2.0",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^6.0.1",
    "json-loader": "^0.5.7",
    "mocha": "^8.3.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "sinon": "^9.2.4",
    "sinon-chai": "^3.6.0",
    "source-map-support": "^0.5.19",
    "string-replace-loader": "^2.3.0",
    "ts-mocha": "^7.0.0",
    "ts-nameof": "^5.0.0",
    "ts-node": "^8.10.2",
    "typedoc": "^0.20.36",
    "typescript": "^3.9.9",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-cyclic-dependency-checker": "^0.0.1",
    "webpack-log": "^3.0.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "tslib": "^2.2.0"
  },
  "-vs-binding": {
    "ProjectOpened": [
      "test"
    ]
  }
}
